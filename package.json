{
  "name": "yzb",
  "version": "1.0.0",
  "description": "YZB project for maimiaotech",
  "author": "liuxin <liuxin@maimiaotech.com>",
  "private": true,
  "scripts": {
    "localhost":"cross-env localhost=localhost node build/dev-server.js",
    "dev":"node build/dev-server.js",
    "build": "cross-env NODE_ENV=production node build/build.js",
    "build:local": "cross-env NODE_ENV=production node build/build.js",
    "build:remote": "cross-env NODE_ENV=production REQUEST_ENV=remote node build/build.js && npm run hash",
    "hash": "cross-env NODE_ENV=production REQUEST_ENV=remote node build/get-assets-hash",
    "upload": "gulp upload --src yzb_static/css/ --dest css/ --file css && gulp upload --src yzb_static/js/ --dest js/ --file js",
    "deploy": "npm run build:remote && npm run upload",
    "build:dll": "webpack --config build/webpack.dll.conf.js && gulp upload --src src/assets/lib/ --dest lib/ --file js",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "analyz": "cross-env NODE_ENV=production  npm_config_report=true node build/build.js"
  },
